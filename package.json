{
  "name": "playground-elm",
  "version": "2.1.0",
  "author": "Chris Camel <camel.christophe@gmail.com>",
  "description": "My playground I use for playing with fancy and exciting technologies. This one's for elm.",
  "homepage": "https://github.com/ccamel",
  "license": "MIT",
  "keywords": [
    "playground",
    "elm",
    "web-application",
    "spa",
    "bootstrap",
    "parcel",
    "svg",
    "demo-site"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ccamel/playground-elm"
  },
  "bugs": {
    "url": "https://github.com/ccamel/playground-elm/issues",
    "email": "camel.christophe@gmail.com"
  },
  "engines": {
    "node": "^18.17.1",
    "pnpm": "^8.3.0"
  },
  "targets": {
    "modern": {
      "engines": {
        "browsers": ">= 50%"
      }
    }
  },
  "scripts": {
    "build": "BASE_URL=/playground-elm/ parcel build src/index.html --no-source-maps --public-url ./",
    "serve": "BASE_URL=/ parcel src/index.html",
    "release": "release-it",
    "prettify": "prettier --write . && elm-format src",
    "lint": "elm-review",
    "elm-json": "elm-json"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "bootstrap": "^4.6.2",
    "elm-canvas": "^2.2.4",
    "file-loader": "^6.2.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.7.1",
    "popper.js": "^1.16.1",
    "style-loader": "^4.0.0",
    "tether": "^2.0.0",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@parcel/transformer-elm": "2.12.0",
    "elm": "^0.19.1",
    "elm-analyse": "^0.16.5",
    "elm-format": "^0.8.7",
    "elm-hot": "^1.1.6",
    "elm-json": "^0.2.13",
    "elm-review": "^2.11.2",
    "node-elm-compiler": "^5.0.6",
    "parcel": "^2.12.0",
    "posthtml-expressions": "^1.11.3",
    "prettier": "^3.3.0",
    "release-it": "^17.3.0",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^5.0.7"
  }
}
